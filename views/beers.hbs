


<div class="flex">
{{#each beersFromApi}}


  <div class="card">
    <img class="card-img-top" src="{{this.image_url}}" alt="{{this.name}} {{this.lastName}}"/>
    <div class="card-body">
      <h5 class="card-title">{{this.name}} </h5>
      <p class="card-text">{{this.description}}</p>
      <p class="card-text">{{this.tagline}}</p>
    </div>
  </div>


{{/each}}
</row>