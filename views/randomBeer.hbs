  <div class="randomBeer">
    <img class="img" src="{{this.image_url}}" alt="{{this.name}} {{this.lastName}}"/>
    <div class="text">
    <h1>{{this.name}}</h1>
    <h6>{{this.tagline}}</h6>
<p>{{this.description}}</p>
<p> <spam>Food Paring</spam></p>
<ul>
{{#each this.food_pairing}}
<li>
{{this}}</li>
{{/each}}
</ul>
<p> <spam>Brewer tips: </spam>{{this.brewers_tips}}</p>
    </div>

